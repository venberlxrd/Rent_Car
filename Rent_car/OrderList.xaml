<Window x:Class="Rent_car.OrderList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rent_car"
        mc:Ignorable="d"
        Title="Список заказов" Height="300" Width="650">
    <Grid>
        <Button x:Name="button" Content="Назад" HorizontalAlignment="Left" Margin="558,191,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <DataGrid x:Name="OrderLi" HorizontalAlignment="Left" Height="176" Margin="10,10,0,0" VerticalAlignment="Top" Width="623" Loaded="OrderLi_Loaded" MouseDoubleClick="OrderLi_MouseDoubleClick" SelectionChanged="OrderLi_SelectionChanged"/>
        <Button x:Name="button1" Content="Создать" HorizontalAlignment="Left" Margin="475,191,0,0" VerticalAlignment="Top" Width="75" Click="button1_Click"/>

    </Grid>
</Window>
