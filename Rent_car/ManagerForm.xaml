        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rent_car"
        mc:Ignorable="d"
        Title="Менеджер" Height="300" Width="600">
    <Grid>
        <Button x:Name="button" Content="Список заказов" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="148" Click="button_Click" Height="52"/>
        <Button x:Name="button1" Content="Список клиентов" HorizontalAlignment="Left" Margin="10,68,0,0" VerticalAlignment="Top" Width="148" Height="47" Click="button1_Click"/>
        <Button x:Name="button2" Content="Список автомобилей" HorizontalAlignment="Left" Margin="10,121,0,0" VerticalAlignment="Top" Width="148" Height="43" Click="button2_Click" MouseDoubleClick="button2_MouseDoubleClick"/>
        <Button  x:Name="button3" Content="Назад" HorizontalAlignment="Left" Margin="507,239,0,0" VerticalAlignment="Top" Width="75" Click="button3_Click"/>

    </Grid>
</Window>
